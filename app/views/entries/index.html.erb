<div class="container">
<h1>All Bids</h1>
<div>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Link</th>
      <th scope="col">Budget</th>
      <th scope="col">Deadline</th>
      <th scope="col">Status</th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>

<% @entries.each do |entry| %>
  <tbody>
  <tr>
  <td><%= entry.title %></td>
  <td><%= entry.link %></td>
  <td><%= entry.budget %></td>
  <td><%= entry.deadline %></td>
  <td><%= entry.status %></td>
  <td><%= button_to "Delete", entry, method: :delete, data: { confirm: "Are you sure you want to delete this Bid ?"}, class: "btn btn-danger" %></td>
  <td><div class="btn btn-success"><%= link_to "Update", edit_entry_path(entry) %></div></td>
  </tr>
  </tbody>
  <% end %>
</div>

</table>
</div>
